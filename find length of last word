  int n=s.size();
        int c=0;

        for(int j=n-1;j>=0;j--){
            if(s[j]!=' '){
                break;
            }
            else{
                n--;
            }
        }

        for(int i=n-1;i>=0;i--){
            if(s[i]==' '){
                break;
            }
            else{
                c++;
            }
        }
        
        return c;
